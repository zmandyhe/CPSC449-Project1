<?xml version="1.0" encoding="UTF-8"?>
<playlist version="1" xmlns="http://xspf.org/ns/0/">
    {% for val in values %}
    <trackList>
        <track>
          <creator>{{val.creator}}</creator>
          <location>{{val.location}}</location>
        </track>
    </trackList>
    {% endfor %}
</playlist>