<?xml version="1.0" encoding="UTF-8"?>
<playlist version="1" xmlns="http://xspf.org/ns/0/">
    <trackList>
        {% for val in values %}
        <track>
          <title>{{val.title}}</title>
          <album>{{val.album}}</album>
          <location>{{val.location}}</location>
        </track>
        {% endfor %}
    </trackList>
</playlist>